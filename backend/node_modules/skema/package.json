{
  "name": "skema",
  "version": "5.2.2",
  "description": "Skema is the collection of common abstract methods for validatiors and setters.",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "nyc ava --verbose --timeout=10s",
    "test-no-cov": "ava --verbose --timeout=10s",
    "prepublish": "npm run build",
    "report-cov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/node-skema.git"
  },
  "keywords": [
    "skema",
    "schema",
    "validators",
    "default",
    "setters",
    "check",
    "validate"
  ],
  "engines": {
    "node": ">=4"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/node-skema/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.16.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.18.0",
    "nyc": "^11.1.0",
    "codecov": "^3.0.0"
  },
  "dependencies": {
    "err-object": "^2.0.0",
    "lodash.foreach": "^4.5.0",
    "make-array": "^1.0.1",
    "promise.extra": "^1.2.0"
  }
}
